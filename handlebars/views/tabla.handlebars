if(productos.length !== 0) { 
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th scope="col">id</th> <th scope="col">producto</th> <th scope="col">precio</th> <th scope="col">url</th> </tr>
            for (const producto in productos ) { 
            <tr>
                <td>{{id}}</td>
                <td>{{producto}}</td>
                <td>{{precio}}</td>
                <td<img src="{{url}}"></td>
            </tr>
             } 
        </table>
    </div>
} else{  
   <h3 style="color: black">no existen datos</h3>
}